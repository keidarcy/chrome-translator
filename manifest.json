{
  "manifest_version": 2,
  "name": "shopify Janpon helper",
  "version": "0.1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/app.js"],
      "css": ["dist/styles.css"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "translate this page",
    "default_icon": {
      "32": "images/icon32.png"
    }
  }
}
